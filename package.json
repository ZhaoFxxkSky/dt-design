{
  "name": "@dtjoy/dt-design",
  "version": "1.0.2",
  "description": "react-component",
  "repository": "https://github.com/ZhaoFxxkSky/dt-design",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "test": "jest",
    "docs:build": "dumi build",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es-fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \".dumi/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:css-fix": "stylelint \"src/**/*.{css,less,scss}\" \".dumi/**/*.{css,less,scss}\" --fix",
    "lint:css": "stylelint \"src/**/*.{css,less,scss}\" \".dumi/**/*.{css,less,scss}\"",
    "lint:es": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \".dumi/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "deploy": "npm run docs:build && gh-pages -d docs-dist",
    "release": "./scripts/release.sh"
  },
  "author": {
    "name": "夕颜",
    "email": "wx-zhaog@dtjoy.com",
    "url": "https://github.com/ZhaoFxxkSky/dt-design"
  },
  "license": "MIT",
  "files": [
    "lib",
    "esm"
  ],
  "sideEffects": [
    "esm/**/style/*",
    "lib/**/style/*",
    "**/style/*"
  ],
  "keywords": [
    "react",
    "react-component",
    "ui-library",
    "typescript",
    "ant-design"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "antd": "^4.x",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.1",
    "@types/react": "^18.0.0",
    "@types/react-resizable": "^3.0.8",
    "@types/react-syntax-highlighter": "~15.5.13",
    "@types/shortid": "^0.0.31",
    "@types/showdown": "^1.9.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "ant-design-testing": "^1.1.0",
    "babel-plugin-import": "^1.13.8",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^2.2.12",
    "eslint": "^8.23.0",
    "father": "~4.1.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ko-lint-config": "2.2.21",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rc-motion": "2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "standard-version": "^9.5.0",
    "stylelint": "^14.9.1",
    "ts-jest": "^29.0.3",
    "typescript": "~4.5.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.3",
    "@dtinsight/dt-utils": "^1.3.1",
    "@rc-component/table": "^1.9.0",
    "classnames": "^2.2.6",
    "immer": "~10.1.1",
    "lodash-es": "^4.17.21",
    "rc-drawer": "~5.1.0",
    "rc-resize-observer": "^1.4.3",
    "rc-util": "^5.44.4",
    "rc-virtual-list": "^3.4.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "rc-motion": "2.6.2"
  }
}
